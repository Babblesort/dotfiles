{
  "window.zoomLevel": 1,
  "explorer.openEditors.visible": 0,
  "workbench.editor.showIcons": false,
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "Source Code Pro",
  "editor.fontSize": 16,
  "editor.renderWhitespace": "none",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 60,
  "files.exclude": {
    "**/karma_html": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true
  },
  "editor.snippetSuggestions": "top",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.enablePreview": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "terminal.integrated.fontFamily": "Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.cursorStyle": "line",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.vscode": true
  },
  "editor.autoIndent": true,
  "editor.renderIndentGuides": false,
  "guides.normal.color.dark": "rgba(60, 60, 60, 0.3)",
  "guides.active.color.dark": "rgba(128, 128, 0, 0.45)",
  "guides.active.color.light": "rgba(200, 100, 100, 0.95)",
  "guides.indent.showFirstIndentGuides": false,
  "guides.stack.enabled": false,
  "guides.sendUsagesAndStats": false,
  "editor.cursorBlinking": "solid",
  "workbench.colorCustomizations": {
    "editorBracketMatch.background": "#65700040",
    "editorBracketMatch.border": "#404040F0",
    "editorLineNumber.foreground": "#303050FF",
    "editorLineNumber.activeForeground": "#9090E0FF",
    "editor.lineHighlightBorder": "#40404020",
    "editorCursor.foreground": "#909090A0",
    "scrollbarSlider.activeBackground": "#a0a0a020",
    "scrollbarSlider.background": "#a0a0a020",
    "scrollbarSlider.hoverBackground": "#a0a0a020"
  },
  "editor.minimap.renderCharacters": true,
  "files.autoSave": "onFocusChange",
  "editor.tabSize": 2,
  "editor.codeLens": false,
  "workbench.fontAliasing": "default",
  "workbench.startupEditor": "none",
  "breadcrumbs.enabled": true,
  "eslint.autoFixOnSave": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "references.preferredLocation": "view",
  "editor.gotoLocation.multiple": "goto",
  "editor.renderLineHighlight": "all",
  "editor.suggest.maxVisibleSuggestions": 5,
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "debug.node.autoAttach": "on",
  "debug.toolBarLocation": "docked",
  "workbench.panel.defaultLocation": "right"
}
